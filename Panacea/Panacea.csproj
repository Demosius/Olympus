  <ItemGroup>
    <Content Include="Panacea.ico" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Morpheus\Morpheus.csproj" />
    <ProjectReference Include="..\Uranus\Uranus.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\application--arrow.png" />
    <Resource Include="Resources\arrow-transition-090.png" />
    <Resource Include="Resources\arrow-transition-270.png" />
    <Resource Include="Resources\arrow-transition.png" />
    <Resource Include="Resources\balance--plus.png" />
    <Resource Include="Resources\calculator--plus.png" />
    <Resource Include="Resources\clock--arrow.png" />
    <Resource Include="Resources\cross.png" />
    <Resource Include="Resources\csv.png" />
    <Resource Include="Resources\database-import.png" />
