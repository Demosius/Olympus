<views:CatPage x:Class="Prometheus.Views.Pages.EquipmentPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:views="clr-namespace:Prometheus.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EquipmentPage">

    <Page.Resources>
        <ResourceDictionary Source="/Morpheus;component/Themes/Dark/Orange/MSControls.Core.Implicit.xaml" />
    </Page.Resources>

    <Grid Background="{StaticResource Brush04}">
        <TextBlock VerticalAlignment="Center"
                   HorizontalAlignment="Center"
                   TextAlignment="Center"
                   TextWrapping="Wrap">
            <Run Foreground="{StaticResource Brush01}"
                 FontSize="14" FontWeight="Bold"
                 >Page Not Completed</Run>    
            <LineBreak></LineBreak>
            <Run Foreground="{StaticResource Brush03}">This page would have been used to directly manage data related to Warehouse Equipment - such as forklifts and associated checklists.</Run>    
        </TextBlock>
    </Grid>
</views:CatPage>
